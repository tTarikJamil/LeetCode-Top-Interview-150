class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
       strs.sort()
       first = strs[0]
       last = strs[-1]
       i,j = 0,0
       result = ""
       while i < len(first) and j < len(last):
           if first[i] == last[j]:
               result += first[i]
               i+=1
               j+=1
           else:
               break
       return result
