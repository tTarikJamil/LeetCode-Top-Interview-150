class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        dic_s = {}
        dic_t = {}
        
        for i in s:
            dic_s[i] = dic_s.get(i,0) + 1

        for i in t:
            dic_t[i] = dic_t.get(i,0) + 1

        if dic_s == dic_t:
            return True
        else:
            return False